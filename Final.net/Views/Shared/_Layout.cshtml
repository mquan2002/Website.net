    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>@ViewData["Title"]</title>

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css2?family=Cairo:wght@200;300;400;600;900&display=swap" rel="stylesheet">

    @* bootrap 5 *@
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- Css Styles -->
    <link rel="stylesheet" href="../LayoutDomino/css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="../LayoutDomino/css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="../LayoutDomino/css/elegant-icons.css" type="text/css">
    <link rel="stylesheet" href="../LayoutDomino/css/nice-select.css" type="text/css">
    <link rel="stylesheet" href="../LayoutDomino/css/jquery-ui.min.css" type="text/css">
    <link rel="stylesheet" href="../LayoutDomino/css/owl.carousel.min.css" type="text/css">
    <link rel="stylesheet" href="../LayoutDomino/css/slicknav.min.css" type="text/css">
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.9.4/dist/leaflet.css" />
    <link rel="stylesheet" href="../LayoutDomino/css/style.css" type="text/css">

    @RenderSection("Scripts", required: false) <!-- Khu vực dành cho script -->

</head>

<body>
    <!-- Include Header -->
    @Html.Partial("Header")

    <!-- Main Content -->
    <main role="main" class="container">
        @RenderBody()
    </main>

    <!-- Include Footer -->
    @Html.Partial("Footer")

    <!-- Js Plugins -->
    <script src="../LayoutDomino/js/jquery-3.3.1.min.js"></script>
    <script src="../LayoutDomino/js/bootstrap.min.js"></script>
    <script src="../LayoutDomino/js/jquery.nice-select.min.js"></script>
    <script src="../LayoutDomino/js/jquery-ui.min.js"></script>
    <script src="../LayoutDomino/js/jquery.slicknav.js"></script>
    <script src="../LayoutDomino/js/mixitup.min.js"></script>
    <script src="../LayoutDomino/js/owl.carousel.min.js"></script>
    <script src="../LayoutDomino/js/main.js"></script>

    @* bootrap 5 *@
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js"></script>
    <!--Start of Tawk.to Script-->
